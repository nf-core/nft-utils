nextflow_pipeline {

    name "Test removeNextflowVersion"
    script "./main.nf"
    tag "removeNextflowVersion"

    test("removeNextflowVersion") {
        when {
            params {
                outdir = "$outputDir"
            }
        }

        then {
          assert snapshot(removeNextflowVersion("$outputDir/pipeline_info/nf_core_pipeline_software_mqc_versions.yml")).match()
        }
    }
}
